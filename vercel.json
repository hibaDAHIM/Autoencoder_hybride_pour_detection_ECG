{
  "version": 2,
  "builds": [
    {
      "src": "ecg_backend/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "ecg_backend/app.py"
    }
  ],
  "env": {
    "PYTHONPATH": "ecg_backend"
  },
  "functions": {
    "ecg_backend/app.py": {
      "runtime": "python3.9"
    }
  }
}